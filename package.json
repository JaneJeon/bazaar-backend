{
  "private": true,
  "scripts": {
    "start": "node -r dotenv/config app",
    "watch": "DEBUG=knex nodemon -r dotenv/config app --trace-sync-io",
    "secret": "node -p \"require('uid-safe').sync(24)\"",
    "migrate": "DEBUG=knex node -r dotenv/config node_modules/.bin/knex migrate:latest --knexfile ./config/database.js",
    "rollback": "DEBUG=knex node -r dotenv/config node_modules/.bin/knex migrate:rollback --knexfile ./config/database.js",
    "seed": "DEBUG=knex node -r dotenv/config node_modules/.bin/knex seed:run --knexfile ./config/database.js",
    "migrate-email": "node scripts/email-templates",
    "update-env": "node scripts/update-env",
    "clean": "rm -f *.log; rm -rf node_modules"
  },
  "dependencies": {
    "aws-sdk": "^2.395.0",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "db-errors": "^0.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^3.3.2",
    "helmet": "^3.15.0",
    "http-assert": "^1.4.0",
    "ioredis": "^4.6.2",
    "knex": "^0.16.3",
    "multer": "^1.4.1",
    "normalize-email": "^1.1.1",
    "objection": "^1.5.3",
    "objection-db-errors": "^1.0.0",
    "objection-password-argon2": "^2.1.0",
    "objection-soft-delete": "^1.0.5",
    "objection-visibility": "^0.4.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "pluralize": "^7.0.0",
    "rate-limit-redis": "^1.6.0",
    "sharp": "^0.21.3",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "debug": "^4.1.1",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.3",
    "supertest": "^3.4.2"
  },
  "prettier": {
    "semi": false
  }
}
